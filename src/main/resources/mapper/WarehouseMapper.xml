<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.African411.dao.WarehouseMapper">
    <resultMap id="BaseResultMap" type="com.African411.domain.Warehouse">
        <id column="warehouse_id" jdbcType="INTEGER" property="warehouseId"/>
        <result column="id" jdbcType="INTEGER" property="id"/>
        <result column="quantity" jdbcType="DECIMAL" property="quantity"/>
    </resultMap>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        delete
        from warehouse
        where warehouse_id = #{warehouseId,jdbcType=INTEGER}
    </delete>
    <insert id="insert" parameterType="com.African411.domain.Warehouse">
        <selectKey keyProperty="id" order="AFTER"
                   resultType="java.lang.Integer">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into warehouse (warehouse_id, quantity)
        values (#{warehouseId,jdbcType=INTEGER}, #{quantity,jdbcType=DECIMAL})
    </insert>
    <update id="updateByPrimaryKey"
            parameterType="com.African411.domain.Warehouse">
        update warehouse
        set id       = #{id,jdbcType=INTEGER},
            quantity = #{quantity,jdbcType=DECIMAL}
        where warehouse_id = #{warehouseId,jdbcType=INTEGER}
    </update>
    <select id="selectByPrimaryKey" parameterType="java.lang.Integer"
            resultMap="BaseResultMap">
        select warehouse_id, id, quantity
        from warehouse
        where warehouse_id = #{warehouseId,jdbcType=INTEGER}
    </select>
    <select id="selectAll" resultMap="BaseResultMap">
        select warehouse_id, id, quantity
        from warehouse
    </select>
</mapper>